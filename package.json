{
  "name": "croc-io",
  "private": true,
  "scripts": {
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,liquid,mdx,html}\"",
    "serve": "rm -rf _site && npx @11ty/eleventy --serve --quiet & npx tailwindcss -i ./app/styles/globals.css -o ./_site/globals.css --watch",
    "build": "rm -rf _site && ELEVENTY_ENV=production npx @11ty/eleventy && npx tailwindcss -i ./app/styles/globals.css -o ./_site/globals.css --minify"
  },
  "dependencies": {
    "@11ty/eleventy": "^2.0.0",
    "@11ty/eleventy-img": "^3.0.0",
    "@fortawesome/fontawesome-pro": "^6.2.1",
    "@fortawesome/fontawesome-svg-core": "^6.2.1",
    "@fortawesome/free-brands-svg-icons": "^6.2.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@mdx-js/react": "^2.2.1",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.5.0",
    "@tailwindcss/line-clamp": "^0.4.0",
    "@tailwindcss/typography": "^0.5.2",
    "autoprefixer": "^10.4.13",
    "babel-plugin-prismjs": "^2.1.0",
    "cheerio": "^1.0.0-rc.12",
    "moment": "^2.29.4",
    "postcss": "^8.4.21",
    "prismjs": "^1.29.0",
    "react": "^18.2.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "slugify": "^1.6.5",
    "tailwindcss": "^3.2.4"
  },
  "devDependencies": {
    "@types/react-copy-to-clipboard": "^5.0.4",
    "netlify-cli": "^12.9.2",
    "prettier": "^2.8.3"
  }
}
